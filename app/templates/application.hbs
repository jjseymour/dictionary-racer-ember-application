<nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
    {{#link-to 'index' class="navbar-brand"}}Home{{/link-to}}
    </div>

    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-2">
      <ul class="nav navbar-nav">
        <li class="active" id="puzzles">{{link-to 'Puzzles' 'puzzles'}}</li>
        <li class="active" id="entries">{{link-to 'Entries' 'entries'}}</li>
        <li class="active" id="users">{{link-to 'Users' 'users'}}</li>
        {{#if session.isAuthenticated}}
          <li class="active" style="postition:absolute; right:-165%;" id="userEmail"><a href="#">Hi, {{sessionAccount.account.email}}</a></li>
          <li class="active" style="postition:absolute; right:-165%;" id="logOut"><a href="#" {{action 'invalidateSession'}}>Logout</a></li>
        {{else}}
          <li class="active" style="postition:absolute; right:-165%;" id="logIn">{{#link-to 'login'}}Log In{{/link-to}}</li>
          <li class="active" style="postition:absolute; right:-165%;" id="SignUp">{{#link-to 'signup'}}Sign Up{{/link-to}}</li>
        {{/if}}
      </ul>
    </div>
  </div>
</nav>


